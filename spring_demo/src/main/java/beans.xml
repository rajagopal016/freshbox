<?xml version = "1.0" encoding = "UTF-8"?>
<beans xmlns = "http://www.springframework.org/schema/beans"
	xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation = "http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
	<bean class="com.to.Hello" id="h1">
		<property name="message" value="Welcome to Spring"></property>
	</bean>
	<bean class="com.to.Hello" id="h2">
		<property name="message" value="Welcome to Autumn"></property>
	</bean>
	
	<bean class="com.to.Employee" id="e1">
		<property name="id" value="123"></property>
		<property name="name" value="abc"></property>
	</bean>
	
	<bean class="com.to.Employee" id="e2">
		<constructor-arg name="id" value="456"></constructor-arg>
		<constructor-arg name="name" value="def"></constructor-arg>
	</bean>
</beans>

